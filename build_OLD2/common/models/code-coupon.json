{
  "name": "CodeCoupon",
  "plural": "code-coupons",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "code": {
      "type": "string",
      "required": true
    },
    "value": {
      "type": "number",
      "required": false,
      "default": 0
    },
    "active": {
      "type": "boolean"
    },
    "discount": {
      "type": "number"
    },
    "isPercentage": {
      "type": "boolean",
      "default": false
    },
    "quantity": {
      "type": "number",
      "default": 1
    }
  },
  "validations": [],
  "relations": {
    "brand": {
      "type": "belongsTo",
      "model": "Brand",
      "foreignKey": "brandId"
    },
    "userCoupons": {
      "type": "hasMany",
      "model": "UserCoupon"
    }
  },
  "acls": [],
  "methods": {}
}
