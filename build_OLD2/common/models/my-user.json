{
  "name": "MyUser",
  "plural": "my-users",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "email": {
      "type": "string"
    },
    "uidAuth": {
      "type": "string",
      "index": true
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "identification": {
      "type": "string"
    },
    "docType": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "birth": {
      "type": "date"
    },
    "avatar": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "provider": {
      "type": "string"
    },
    "brandId": {
      "type": "number",
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "myRoles": {
      "type": "hasMany",
      "model": "MyRole",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      },
      "through": "AssinedRole"
    },
    "motivators": {
      "type": "hasMany",
      "model": "Motivator",
      "foreignKey": "userId"
    },
    "brand": {
      "type": "belongsTo",
      "model": "Brand",
      "foreignKey": "brandId"
    }
  },
  "acls": [],
  "methods": {}
}
