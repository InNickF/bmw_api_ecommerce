{
  "name": "Product",
  "plural": "products",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "stock": {
      "type": "number",
      "required": true
    },
    "price": {
      "type": "number",
      "dataType": "decimal"
    },
    "description": {
      "type": "string",
      "dataType": "text"
    },
    "sku": {
      "type": "string",
      "required": true
    },
    "warrantyYear": {
      "type": "number",
      "default": 2
    },
    "weight": {
      "type": "number"
    },
    "color": {
      "type": "string"
    },
    "check": {
      "type": "boolean"
    },
    "accessories": {
      "type": "boolean"
    },
    "weightVolume": {
      "type": "number",
      "dataType": "decimal",
      "precision": 10,
      "scale": 2
    },
    "size": {
      "type": "string"
    },
    "high": {
      "type": "number"
    },
    "width": {
      "type": "number"
    },
    "long": {
      "type": "number"
    },
    "yearStart": {
      "type": "number"
    },
    "yearEnd": {
      "type": "number"
    },
    "sound": {
      "type": "string"
    },
    "priceWithTax": {
      "type": "number"
    },
    "priceWithoutTax": {
      "type": "number"
    },
    "make": {
      "type": "string"
    },
    "installation": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "deplete": {
      "type": "number",
      "required": true,
      "default": 5
    },
    "tagText": {
      "type": "number",
      "required": true,
      "default": 5
    },
    "discount": {
      "type": "boolean",
      "default": false
    },
    "highlight": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "intent": {
      "type": "number"
    },
    "isFather": {
      "type": "boolean",
      "default": true
    },
    "totalStock": {
      "type": "number"
    },
    "discountPercentage": {
      "type": "number"
    },
    "initDateDiscount": {
      "type": "date"
    },
    "endDateDiscount": {
      "type": "date"
    },
    "gender": {
      "type": "string"
    },
    "scale": {
      "type": "string"
    },
    "hex": {
      "type": "string"
    },
    "indexRin": {
      "type": "string"
    },
    "productBrand": {
      "type": "string"
    },
    "runflat": {
      "type": "boolean",
      "default": false
    },
    "location": {
      "type": "string"
    },
    "collection": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "material": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "productCategory": {
      "type": "belongsTo",
      "model": "ProductCategory",
      "foreignKey": "productCategoryId"
    },
    "relatedProducts": {
      "type": "hasMany",
      "model": "RelatedProduct"
    },
    "storeProducts": {
      "type": "hasMany",
      "model": "StoreProduct"
    },
    "imageProducts": {
      "type": "hasMany",
      "model": "ImageProduct"
    },
    "attributevalues": {
      "type": "hasMany",
      "model": "AttributeValue"
    },
    "complements": {
      "type": "hasMany",
      "model": "Complement"
    },
    "vehicleType": {
      "type": "belongsTo",
      "model": "VehicleType",
      "foreignKey": "vehicleTypeId"
    },
    "brand": {
      "type": "belongsTo",
      "model": "Brand",
      "foreignKey": "brandId"
    },
    "productVariations": {
      "type": "hasMany",
      "model": "ProductVariation",
      "foreignKey": "productId"
    },
    "skuVariations": {
      "type": "hasMany",
      "model": "SkuVariation"
    },
    "skuChildren": {
      "type": "hasOne",
      "model": "SkuVariation",
      "foreignKey": "productChildrenId"
    }
  },
  "acls": [],
  "methods": {}
}
