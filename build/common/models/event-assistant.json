    "user": {
      "type": "belongsTo",
      "model": "MyUser",
      "foreignKey": "userId"
    },
    "brand": {
      "type": "belongsTo",
      "model": "Brand",
      "foreignKey": "brandId"
    }
  },
  "acls": [],
  "methods": {}
}
