{
  "name": "EventAssistant",
  "plural": "event-assistants",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "username": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
