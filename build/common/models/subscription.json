{
  "name": "Subscription",
  "plural": "suscriptions",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "email": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "boolean",
      "required": true,
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "MyUser",
      "foreignKey": "userId"
    },
    "brand": {
      "type": "belongsTo",
      "model": "Brand",
      "foreignKey": "brandId"
    }
  },
  "acls": [],
  "methods": {}
}
