{
  "name": "EventComment",
  "plural": "event-comments",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "title": {
      "type": "string"
    },
    "body": {
      "type": "string",
      "required": true,
      "dataType": "text"
    },
    "score": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "event": {
      "type": "belongsTo",
      "model": "Event",
      "foreignKey": "eventId"
    },
    "user": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}
}
